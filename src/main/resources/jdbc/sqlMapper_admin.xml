<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="partyDuo.com.mapper.AdminMapper">
	
	
	<select id="selectAll" resultType="partyDuo.com.model.AdminVO">
		select * from admin order by admin_id desc 
	</select>
	
	<select id="selectOne"
		resultType="partyDuo.com.model.AdminVO">
		select * from admin where id=#{id}
	</select>

</mapper>
